local repo = "https://raw.githubusercontent.com/deividcomsono/Obsidian/main/"
local Library = loadstring(game:HttpGet(repo .. "Library.lua"))()

local Toggles = Library.Toggles

local Window = Library:CreateWindow({
    Title = "Radiology Autofarm",
    Footer = "by MasterZ",
    Icon = 0,
    NotifySide = "Right",
    ShowCustomCursor = true,
})

local Tabs = {
    Main = Window:AddTab("Main", "toggle-left"),
}

local GroupBox = Tabs.Main:AddLeftGroupbox("Autofarm")

local autofarmRadiology = false

GroupBox:AddToggle("RadiologyToggle", {
    Text = "Autofarm Radiology",
    Default = false,
    Tooltip = "Wedgera ‚ûú X-Ray ‚ûú Computer ‚ûú Attachment ‚ûú Wedge ‚ûú Torso ‚ûú Sheet",
    Callback = function(value)
        autofarmRadiology = value
        if value then
            task.spawn(function()
                local TweenService = game:GetService("TweenService")
                local Players = game:GetService("Players")
                local player = Players.LocalPlayer
                local char = player.Character or player.CharacterAdded:Wait()
                local hrp = char:WaitForChild("HumanoidRootPart")

                local function tweenTo(part)
                    if not part then return end
                    local goal = { CFrame = part.CFrame + Vector3.new(0, 3, 0) }
                    local tween = TweenService:Create(hrp, TweenInfo.new(1, Enum.EasingStyle.Linear), goal)
                    tween:Play()
                    tween.Completed:Wait()
                end

                local room
                for _, r in ipairs(workspace:WaitForChild("RadiologyRooms"):GetChildren()) do
                    if r:IsA("Model") and r.Name == "RadiologyRoom1" then
                        room = r
                        break
                    end
                end
                if not room then warn("RadiologyRoom1 tidak ditemukan.") return end

                local wedgera = room:FindFirstChild("Wedgera", true)
                if not wedgera then warn("Wedgera tidak ditemukan.") return end
                tweenTo(wedgera)

                local clickPart
                local machineRoot = room:FindFirstChild("X-Ray (with CXR and table) Machine", true)
                if machineRoot then
                    local xrayMachine = machineRoot:FindFirstChild("X-Ray Machine")
                    if xrayMachine then
                        for _, obj in ipairs(xrayMachine:GetDescendants()) do
                            if obj:IsA("ClickDetector") then
                                clickPart = obj.Parent
                                break
                            end
                        end
                    end
                end
                if not clickPart then warn("ClickDetector tidak ditemukan.") return end

                local prompt
                local xrayBtn = room:FindFirstChild("X-Ray Button")
                if xrayBtn then
                    for _, part in ipairs(xrayBtn:GetChildren()) do
                        local p = part:FindFirstChildWhichIsA("ProximityPrompt")
                        if p then
                            prompt = p
                            break
                        end
                    end
                end
                if not prompt then warn("Prompt tidak ditemukan.") return end

                while autofarmRadiology and not prompt.Enabled do
                    pcall(function()
                        fireclickdetector(clickPart:FindFirstChildWhichIsA("ClickDetector"))
                    end)
                    task.wait(0.5)
                end
                if not autofarmRadiology then return end
                print("‚úÖ Prompt aktif!")

                local computer = room:FindFirstChild("radiologycomputer2")
                if not computer then warn("radiologycomputer2 tidak ditemukan.") return end
                tweenTo(computer)
                local compPrompt = computer:FindFirstChildWhichIsA("ProximityPrompt", true)
                if compPrompt then
                    fireproximityprompt(compPrompt)
                    print("‚úÖ Prompt di radiologycomputer2 berhasil di-trigger.")
                else
                    warn("Prompt di radiologycomputer2 tidak ditemukan.")
                end

                local foundPartWithPrompt = nil
                for _, part in ipairs(room:GetChildren()) do
                    if part:IsA("BasePart") then
                        local attachment = part:FindFirstChildWhichIsA("Attachment", true)
                        if attachment then
                            local attPrompt = attachment:FindFirstChildWhichIsA("ProximityPrompt", true)
                            if attPrompt then
                                foundPartWithPrompt = part
                                tweenTo(part)
                                task.wait(0.3)
                                fireproximityprompt(attPrompt)
                                print("‚úÖ Attachment Prompt berhasil di-trigger.")
                                break
                            end
                        end
                    end
                end

                if not foundPartWithPrompt then
                    warn("‚ùå Tidak menemukan Part dengan Attachment Prompt.")
                end

                -- Equip Wedge
                local wedgeTool = player.Backpack:FindFirstChild("Wedge")
                if wedgeTool then
                    player.Character.Humanoid:EquipTool(wedgeTool)
                    print("üß± Tool 'Wedge' berhasil di-equip.")
                else
                    warn("‚ùå Tool 'Wedge' tidak ditemukan.")
                end

                -- Torso ‚ûú Trigger
                local torso = room:FindFirstChild("PatientRadiology", true)
                if torso then torso = torso:FindFirstChild("Torso") end
                if torso and torso:IsA("BasePart") then
                    tweenTo(torso)
                    local torsoPrompt = torso:FindFirstChildWhichIsA("ProximityPrompt", true)
                    if torsoPrompt then
                        fireproximityprompt(torsoPrompt)
                        print("‚úÖ Prompt di Torso berhasil di-trigger.")
                    else
                        warn("‚ùå Prompt di Torso tidak ditemukan.")
                    end
                else
                    warn("‚ùå Torso tidak ditemukan.")
                end

                -- ‚úÖ NEW STEP: X-Ray Table ‚ûú dd ‚ûú ProximityPrompt
                local ddPromptPart = nil
                local xrayTableModel = machineRoot:FindFirstChild("X-Ray Table", true)
                if xrayTableModel then
                    local ddPart = xrayTableModel:FindFirstChild("dd")
                    if ddPart and ddPart:IsA("BasePart") then
                        local ddPrompt = ddPart:FindFirstChildWhichIsA("ProximityPrompt", true)
                        if ddPrompt then
                            tweenTo(ddPart)
                            task.wait(0.3)
                            fireproximityprompt(ddPrompt)
                            print("‚úÖ Prompt di 'dd' berhasil di-trigger.")
                        else
                            warn("‚ùå ProximityPrompt tidak ditemukan di 'dd'.")
                        end
                    else
                        warn("‚ùå Part 'dd' tidak ditemukan.")
                    end
                else
                    warn("‚ùå X-Ray Table tidak ditemukan.")
                end

            end)
        else
            print("‚ùå Autofarm dimatikan.")
        end
    end
})
